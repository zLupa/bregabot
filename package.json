{
  "name": "bregabot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsup src/*",
    "dev": "tsup-node src/* --watch --onSuccess \"node dist/main.js\"",
    "serve": "node build/main.js",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only src/main.ts"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.2",
    "@discordx/pagination": "^2.0.1",
    "@prisma/client": "3.12.0",
    "discord.js": "^13.6.0",
    "discordx": "^9.5.1",
    "dotenv": "^16.0.0",
    "node-fetch": "^3.2.3",
    "prisma": "^3.12.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0"
  },
  "devDependencies": {
    "@swc/core": "^1.2.165",
    "@types/node": "^17.0.23",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsup": "^5.6.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
